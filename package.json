    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "multer": "^1.4.5-lts.1",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.8.0",
    "pug": "^3.0.2",
    "sequelize": "^6.26.0"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.13",
